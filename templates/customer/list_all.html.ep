% layout 'default';
% title 'All Customers';


	<table>
		<thead>
			<th></th>
			<th>Name</th>
		</thead>
		<tbody>
			% foreach my $customer (@{$customers}){
				<tr>
					<td><a href="<%= url_for('get_customer') %>/<%= $customer->[1] %>" class="btn btn-default">Get</a></td>
					<td><%= $customer->[0] %></td>
				</tr>
			% }
		</tbody>
	</table>